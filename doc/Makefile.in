	$(INSTALL_DATA) afterstep-5.html $(AFTER_DOC_DIR)
	$(INSTALL_DATA) afterstep-6.html $(AFTER_DOC_DIR)
	$(INSTALL_DATA) afterstep-7.html $(AFTER_DOC_DIR)
	$(INSTALL_DATA) afterstep-8.html $(AFTER_DOC_DIR)
	$(INSTALL_DATA) afterstep-9.html $(AFTER_DOC_DIR)
	$(INSTALL_DATA) afterstep-10.html $(AFTER_DOC_DIR)

uninstall.data:
